{%- style -%}
{%- capture style -%}
  /*================ Typography ================*/
  :root {
  --color-bg-body:{{settings.color_body_bg}};
  --color-text-body:{{settings.color_body_text}};
  
  --color-main: {{settings.color_main}};
  --color-secondary: {{settings.color_secondary}};
    
  --color-link: {{settings.color_link}};
  --color-link-hover: {{settings.color_link_hover}};
  
  --color-scrollbar: {{settings.color_scrollbar}};  
  --color-border: {{settings.color_border}};  
  
  /*================ megamenu ================*/
  --color-link-menu:{{settings.color_link_menu}};
  --color-link-menu-heading:{{settings.color_menu_heading}};
  --color-link-menu-sub:{{settings.color_link_menu_sub}};
  --color-link-menu-hover:{{settings.color_link_menu_hover}};
  
  /*================ product item ================*/ 
  --color-name-product:{{settings.color_name_product }};
  --color-name-product-hover:{{settings.color_name_product_hover }};
  --color-star-product:{{settings.color_star_product }};
  
  --color-compare-price-product:{{settings.color_compare_price_product }};
  --color-price-product:{{settings.color_price_product }};
  --color-current-product:{{settings.color_price_current_product }};
  
  --color-cart-product:{{settings.color_cart_product }};
  --color-out-product:{{settings.color_out_product }};
    
  --color-btn-product:{{settings.color_btn_product }};
  --color-btn-product-hover:{{settings.color_btn_product_hover }};
    
  --bg-btn-product:{{settings.bg_btn_product }};
  --bg-btn-product-hover:{{settings.bg_btn_product_hover }};
    
  --color-label-hot:{{settings.color_label_hot }};
  --color-label-new:{{settings.color_label_new }};
  --color-label-sale:{{settings.color_label_sale }};
  --color-label-in:{{settings.color_label_in }};
  --color-label-out:{{settings.color_label_out }};
  
  /*================ Butoom ================*/
  
  --color-btn-text:{{settings.color_btn_text }};
  --color-btn:{{settings.color_btn }};
  --color-border-btn:{{settings.color_border_btn }};
  --color-btn-text_hover:{{settings.color_btn_text_hover }};
  --color-btn-hover:{{settings.color_btn_hover }};
  --color-border-btn-hover:{{settings.color_border_btn_hover }};
   /*================ Butoom Previous & Next ================*/ 
    --color-cl-btn-p-n:{{settings.cl_btn_p_n }};
    --color-bg-btn-p-n:{{settings.bg_btn_p_n }};
    --color-cl-btn-p-n-hover:{{settings.cl_btn_p_n_hover }};
    --color-bg-btn-p-n-hover:{{settings.bg_btn_p_n_hover }};

    --alo-border-radius:{{settings.border}}px;
      --alo-gap:{{settings.gap}}px;
      --alo-row:-{{settings.gap}}px;

      --alo-gap-mobile:{{settings.gap_mobile}}px;
      --alo-row-mobile:-{{settings.gap_mobile}}px;
  
      --alo-container-width:{{settings.cusotm_width}}px;
    
  }
  .hero_canvas:before,
  .mfp-container,.mobile_bkg_show,.mask-overlay{
    cursor: url({{'cursor-close.png' | asset_url}}),pointer !important;
  }
  .frm_search_cat select {
    background-image: url({{'icon_down.png' | asset_url}});
    background-position: right 20px top 50%;
    background-size: auto 6px;
    background-repeat: no-repeat;
  }
  .content-video .number:before{
    -webkit-mask: url({{'icon_1.svg' | asset_url}}) no-repeat center center;
    mask: url({{'icon_1.svg' | asset_url}}) no-repeat center center;
  }
  {%- endcapture -%}
{{ style | strip_newlines }}
{%- endstyle -%}

