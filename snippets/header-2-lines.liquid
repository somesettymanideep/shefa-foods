                {%- render 'list-vertical-menu' -%}
              {% endif %}
            </div>
          {%- endif -%}
        {%- endif -%}
        <div class="ps-static col">
          {%- render 'menu', se_blocks: se_blocks, tp_blocks: tp_blocks -%}
        </div>
