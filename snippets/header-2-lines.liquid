                  </svg>

                  </a>
                </li>
              {% endif %}
            {% endif %}

            {% if section.settings.header_use_account %}
              <li class="signin item-list hide-mobile">
                {% unless customer %}
                  {%- assign page_type = request.page_type | handle -%}
                  <a
                    href="{{ routes.root_url }}account/login"
                    {% if page_type != 'customers-login' %}
                      data-id="#login_pupop"
                    {% endif %}
                    class="push_side header-icon"
                    title="{{'customer.login.log_in' | t}}"
                  >
                    {%- render 'icon-login' -%}
                    <span class="text">
                        <span class="l">{{ 'customer.login.log_in' | t }}</span>
                        <span class="a">{{ 'customer.login.account' | t }}</span>
                    </span>
                  </a>
                {% else %}
                  <a
                    href="{{ routes.account_url }}"
                    class="push_side header-icon"
                    title="{{'customer.login.account' | t}}"
                  >
                    {%- render 'icon-login' -%}
                    <span class="text">
                      <span class="l">{{ customer.name }}</span>
                      <span class="a">{{ 'customer.login.account' | t }}</span>
                    </span>
                  </a>
                {% endunless %}
              </li>
            {% endif %}

