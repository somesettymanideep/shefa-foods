            {% if section.settings.header_use_cart_home %}
              <li class="box-minicart">
                <div class="minicart ">
                  <div class="cart-block  box-has-content">
                    <a
                      href="{{ routes.cart_url }}"
                      class="push_side header-icon"
                      {% if request.page_type != 'cart' %}
                        data-id="#js_cart_popup"
                      {% endif %}
                      title="{{'cart.general.title' | t}}"
                    >
                      {%- render 'icon-cart' -%}
                      <span class="header__counter js-cart-count" data-js-cart-count="{{ cart.item_count }}">
                        {{- cart.item_count -}}
                      </span>
                      <span data-total-price class="total-price">
                        <span class="text">{{ 'cart.general.title' | t }} </span>
                        <span class="js-total-price"> {{ cart.total_price | money }}</span>
                      </span>
                    </a>

                    
                  </div>
                </div>
              </li>
            {% endif %}
          </ul>
        </div>
      </div>
      {% if section.settings.hide_search_mobile == false %}              
        {%- render 'header-search-mobile' -%}
      {% endif %}  
    </div>
  </div>
  <div
    class="{% if settings.vertical_menu_position == '3' %}no-vertical{% endif %} navigationMenu hide-mobile alo-background-color"
    style="--alo-background-color:{{ section.settings.cl_bg_menu }}"
  >
    <div
      class="container {% if section.settings.layout == 'full-width' %}{{section.settings.layout}} {% endif %} {% if section.settings.custom_width %}alo-custom-width{% endif %}"
      {% if section.settings.custom_width %}
        style="--alo-custom-width:{{ section.settings.container_width }}px;"
      {% endif %}
    >
      <div class="row row-0">
        {%- if settings.use_vertical_menu -%}
          {% if settings.vertical_menu_position == '1' or settings.vertical_menu_position == '2' %}
            <div class="col-auto vertical_menu">
              <div class="title_vertical_menu {% if settings.vertical_menu_position == '1' or request.page_type !='index'%}click{% endif %} flex">
